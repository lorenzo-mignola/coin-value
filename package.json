  "devDependencies": {
    "@preact/preset-vite": "^2.0.0",
    "assemblyscript": "^0.18.27",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.12",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.1.3",
    "vite": "^2.1.5"
  }
}